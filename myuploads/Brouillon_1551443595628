  CREATE TABLE docs(
 id SERIAL PRIMARY KEY,
 filename VARCHAR (250) NOT NULL,
 relativename VARCHAR (250) NOT NULL,
 encoding VARCHAR (20) NOT NULL,
 extension VARCHAR (20) NOT NULL,
 path text NOT NULL,
 size int,
 content text,
 "createdAt" TIMESTAMP,
 "updatedAt" TIMESTAMP,
);




